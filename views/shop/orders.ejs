<%- include('../includes/head.ejs') %> 

  <%- include('../includes/navigation.ejs') %>

  <main>
    <% if (orders.length > 0) { %> 
      <ul>
        <% for (const order of orders) { %> 
          <li>
            <h1># <%= order.id %> </h1>
            <ul>
              <% order.products.forEach(product => { %> 
                <li>
                  <%= product.title %> (qty: <%= product.orderItem.quantity %>)
                </li>
              <% }) %> 
            </ul>
          </li>
        <% } %> 
      </ul>
    <% } else { %> 
      <h1>No Orders yet!!!!</h1>
    <% } %> 
  </main>

<%- include('../includes/end.ejs') %>